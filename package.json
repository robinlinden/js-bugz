{
  "name": "toktok-bugz",
  "version": "1.0.0",
  "private": true,
  "description": "TokTok bug and pull request management",
  "author": "iphydf <iphydf@users.noreply.github.com>",
  "license": "ISC",
  "repository": "https://github.com/TokTok/bugz.git",
  "homepage": "https://github.com/TokTok/bugz",
  "bugs": "https://github.com/TokTok/bugz/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc && (tsc -w --preserveWatchOutput & nodemon)",
    "dev": "npm run build:watch",
    "start": "probot run ./lib/index.js",
    "format": "prettier --write '**/*.ts' '**/*.json'",
    "report-coverage": "codecov --disable=gcov",
    "test": "jest --coverage",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "mongodb": "^3.5.6",
    "probot": "^9.11.3"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.18",
    "@types/jest": "^25.2.1",
    "@types/mongodb": "^3.5.11",
    "@types/nock": "^11.1.0",
    "@types/node": "^13.1.0",
    "@typescript-eslint/parser": "^2.29.0",
    "codecov": "^3.6.5",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^25.4.0",
    "nock": "^12.0.3",
    "nodemon": "^2.0.3",
    "prettier": "^2.0.5",
    "ts-jest": "^25.1.0",
    "typescript": "^3.8.3"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
